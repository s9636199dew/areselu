pipeline{
    agent any
    stages{
        stage ('deploy'){
            environment{
                credentials=('awsfile')
            }
           steps {
        sh '''
         ssh -i $awsfile -o StrictHostKeyChecking=no ubuntu@3.21.41.81 'sudo bash -s' < bobbatlu/myscript.sh
         '''


           }
           
           }
        }
    }
